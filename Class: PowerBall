package lotterysimulator;
import java.util.Random;

public class PowerBall 
{
    private int first, second, third, fourth, fifth, sixth;
    
    public PowerBall()
    {
        Random rand = new Random();

        first = rand.nextInt(70) + 1;
        second = rand.nextInt(70) + 1;
        third = rand.nextInt(70) + 1;
        fourth = rand.nextInt(70) + 1;
        fifth = rand.nextInt(70) + 1;
        sixth = rand.nextInt(27) + 1;
    }
    
    public int getFirst()
    {
        return first;
    }
    
    public int getSecond()
    {
        return second;
    }
    
    public int getThird()
    {
        return third;
    }
    
    public int getFourth()
    {
        return fourth;
    }
    
    public int getFifth()
    {
        return fifth;
    }
    
    public int getSixth()
    {
        return sixth;
    }
}
